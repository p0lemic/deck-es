{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "broadway/broadway": "2.3.*",
        "broadway/broadway-bundle": "^0.5.0",
        "broadway/event-store-dbal": "0.5.*",
        "friendsofsymfony/rest-bundle": "^3.0",
        "gesdinet/jwt-refresh-token-bundle": "^0.9.1",
        "jms/serializer-bundle": "^3.4",
        "lexik/jwt-authentication-bundle": "^2.6",
        "monolog/monolog": "^2.0",
        "nelmio/api-doc-bundle": "3.6.*",
        "nelmio/cors-bundle": "^2.1",
        "ramsey/uuid": "^3.8",
        "ramsey/uuid-doctrine": "^1.5",
        "simple-bus/doctrine-dbal-bridge": "^2.0",
        "simple-bus/doctrine-orm-bridge": "^5.0",
        "simple-bus/symfony-bridge": "5.4.*",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/debug": "4.4.7",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.11",
        "symfony/framework-bundle": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/proxy-manager-bridge": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/security-core": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Deck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deck\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/test-pack": "^1.0"
    }
}
